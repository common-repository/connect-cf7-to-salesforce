{
    "name": "mihasicehcek/php_salesforce_soql_builder",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [],
    "autoload": {
        "psr-4": {
            "SalesforceQueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {},
    "scripts": {},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "description": "SOQL Builder"
}
